<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/baseFragment">

    <fragment
        android:id="@+id/baseFragment"
        android:name="com.example.shynmcd.ui.fragment.BaseFragment"
        android:label="base_fragment"
        tools:layout="@layout/base_fragment" >
        <action
            android:id="@+id/action_baseFragment_to_addPlayerFragment"
            app:destination="@id/addPlayerFragment" />
    </fragment>
    <fragment
        android:id="@+id/addPlayerFragment"
        android:name="com.example.shynmcd.ui.fragment.AddPlayerFragment"
        android:label="add_player_layout"
        tools:layout="@layout/add_player_layout" />
</navigation>